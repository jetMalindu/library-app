{
  "compilerOptions": {
    "lib": [
      "es2016"
    ],
    "target": "ES6",
    "sourceMap": true,
    "declaration": true,
    "module": "commonjs",
    "rootDir": ".",
    "outDir": "./dist/",
    "noUnusedParameters": false,
    "noUnusedLocals": true,
    "experimentalDecorators": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "include": ["src/**/*.ts"],
}
